@model List<SOL_WILFREDO_VALVERDE.Models.View.VistaMatricula>
@{
    ViewBag.Title = "ListarMatriculas";
}

<div class="row">

    <div class="col-sm-12">
        <table id="tabla" class="table" style="width:100%">
            <thead>
                <tr>
                    <th>Id Matricula</th>
                    <th>Código Alumno</th>
                    <th>Nombres</th>
                    <th>Apellidos</th>
                    <th>Id Curso</th>
                    <th>Descripción Curso</th>
                    <th>Créditos</th>
                    <th>Nombre Sección</th>
                    <th>Tipo matricula</th>
                    <th>Fecha Matricula</th>
                    <th>Fecha Anulación</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var oElemento in Model)
                {
                    <tr>
                        <td>@oElemento.id_matricula</td>
                        <td>@oElemento.id_alumno</td>
                        <td>@oElemento.nombres</td>
                        <td>@oElemento.apellidos</td>
                        <td>@oElemento.id_curso</td>
                        <td>@oElemento.nombrecurso</td>
                        <td>@oElemento.creditos</td>
                        <td>@oElemento.seccion</td>
                        <td>@oElemento.modalidad</td>
                        <td>@oElemento.fecha_matricula</td>
                        <td>@oElemento.fecha_anulacion</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>

</div>

